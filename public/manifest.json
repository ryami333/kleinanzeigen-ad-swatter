{
  "manifest_version": 3,

  "name": "Kleinanzeigen Ad Swatter",
  "description": "Removes ad listings on Kleinanzeigen",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.kleinanzeigen.de/*"],
      "js": ["content_script.js"]
    }
  ]
}
